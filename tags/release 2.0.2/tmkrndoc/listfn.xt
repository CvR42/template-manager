.set l a long list of nice and different words
index: ${index of $l}, ${index bla $l}
member: ${member of $l}, ${member bla $l}
shift: ${shift $l}
first: ${first $l}
seplist: ${seplist - $l}
prefix: ${prefix z $l}
suffix: ${suffix z $l}
len: ${len $l}
sort: ${sort $l}
sizesort: ${sizesort $l}
rev: ${rev $l}
