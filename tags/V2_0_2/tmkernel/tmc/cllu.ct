.case class constructorbase
    tm_opencons( st );
.if ${len ${nonvirtual ${subclasses $t}}}
    switch( t->tag ){
.foreach c ${nonvirtual ${subclasses $t}}
	case TAG$c:
	    tm_printword( st, "$c" );
.foreach e ${allfields $c}
	    print_${type $c $e}( st, to_$c(t)->$e );
.endforeach
	    break;

.endforeach
	default:
	    FATALTAG( t->tag );
    }
.endif
    tm_closecons( st );
