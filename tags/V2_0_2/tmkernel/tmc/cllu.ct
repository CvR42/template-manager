/* Print an element of type '$t' to file 'f' */
$(stat)void fprint_$t( FILE *f, const $t t )
{
    if( t == $tNIL ){
	fputs( "@ ", f );
	return;
    }
.if ${member $t ${tuplelist}}
    putc( '(', f );
.set first 1
.foreach e ${allfields $t}
.if $(first)
.set first 0
.else
    fputs( ",\n", f );
.endif
    fprint_${type $t $e}( f, t->$e );
.endforeach
    fputs( ")\n", f );
.else
    putc( '(', f );
.if ${len ${subclasses $t}}
    switch( t->tag ){
.foreach c ${subclasses $t}
	case TAG$c:
	    fputs( "$c", f );
.foreach e ${allfields $c}
	    putc( ' ', f );
	    fprint_${type $c $e}( f, to_$c(t)->$e );
.endforeach
	    break;

.endforeach
	default:
	    FATALTAG( t->tag );
    }
.endif
    fputs( ")\n", f );
.endif 
}

