    while( b!=0 && l != $t_listNIL ){
	b--;
	e--;
	l = l->${call linkname $t};
    }
    if( l == $t_listNIL ){
	return new_$t_list();
    }
    nl = rdup_$t( l );
    nl->${call linkname $t} = slice_$t_list( l->${call linkname $t}, 0, (e-1) );
    return nl;
}

